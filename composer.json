    "execut/yii2-base": "*",
    "execut/yii2-navigation": "*",
    "execut/yii2-alias": "*",
    "execut/yii2-seo": "*",
    "execut/yii2-pages": "*",
    "execut/yii2-menu": "*",
    "execut/yii2-settings": "*",
    "execut/yii2-robots-txt": "*",
